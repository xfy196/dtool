{
  "name": "it-tools",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "lint:lint-staged": "lint-staged",
    "commit": "git pull && git add -A && git-cz && git push"
  },
  "dependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/crypto-js": "^4.2.2",
    "@vicons/fluent": "^0.12.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/tabler": "^0.12.0",
    "@vueuse/core": "^11.0.1",
    "bcryptjs": "^2.4.3",
    "colord": "^2.9.3",
    "crypto-js": "^4.2.0",
    "date-fns": "^3.6.0",
    "fuse.js": "^7.0.0",
    "js-base64": "^3.7.7",
    "lodash": "^4.17.21",
    "naive-ui": "^2.39.0",
    "pinia": "^2.2.2",
    "romans": "^2.0.16",
    "ulid": "^2.3.0",
    "uuid": "^10.0.0",
    "validator": "^13.12.0",
    "vue": "^3.4.37",
    "vue-i18n": "^9.14.0",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.1",
    "@commitlint/config-conventional": "^19.4.1",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.4.1",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.12.1",
    "@vitejs/plugin-vue": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.0",
    "cz-git": "^1.9.4",
    "eslint": "^9.9.1",
    "husky": "^9.1.5",
    "install": "^0.13.0",
    "lint-staged": "^15.2.10",
    "pnpm": "^9.9.0",
    "postcss": "^8.4.41",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.77.8",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.3",
    "unplugin-auto-import": "^0.18.2",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.1",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "eslint",
      "prettier --write"
    ],
    "*.{cjs,json}": [
      "eslint",
      "prettier --write"
    ],
    "*.{vue,html}": [
      "prettier --write"
    ],
    "*.{scss,css}": [
      "eslint",
      "prettier --write"
    ],
    "*.md": [
      "eslint",
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  }
}
